<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Clin Interv Aging</journal-id><journal-id journal-id-type="iso-abbrev">Clin Interv Aging</journal-id><journal-id journal-id-type="publisher-id">Clinical Interventions in Aging</journal-id><journal-title-group><journal-title>Clinical Interventions in Aging</journal-title></journal-title-group><issn pub-type="ppub">1176-9092</issn><issn pub-type="epub">1178-1998</issn><publisher><publisher-name>Dove Medical Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">29713151</article-id><article-id pub-id-type="pmc">5909776</article-id><article-id pub-id-type="doi">10.2147/CIA.S158097</article-id><article-id pub-id-type="publisher-id">cia-13-669</article-id><article-categories><subj-group subj-group-type="heading"><subject>Original Research</subject></subj-group></article-categories><title-group><article-title>Design and implementation of an empowerment model to prevent elder abuse: a randomized controlled trial</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Estebsari</surname><given-names>Fatemeh</given-names></name><xref ref-type="aff" rid="af1-cia-13-669">1</xref></contrib><contrib contrib-type="author"><name><surname>Dastoorpoor</surname><given-names>Maryam</given-names></name><xref ref-type="aff" rid="af2-cia-13-669">2</xref><xref ref-type="corresp" rid="c1-cia-13-669"/></contrib><contrib contrib-type="author"><name><surname>Mostafaei</surname><given-names>Davoud</given-names></name><xref ref-type="aff" rid="af3-cia-13-669">3</xref></contrib><contrib contrib-type="author"><name><surname>Khanjani</surname><given-names>Narges</given-names></name><xref ref-type="aff" rid="af4-cia-13-669">4</xref></contrib><contrib contrib-type="author"><name><surname>Khalifehkandi</surname><given-names>Zahra Rahimi</given-names></name><xref ref-type="aff" rid="af5-cia-13-669">5</xref></contrib><contrib contrib-type="author"><name><surname>Foroushani</surname><given-names>Abbas Rahimi</given-names></name><xref ref-type="aff" rid="af6-cia-13-669">6</xref></contrib><contrib contrib-type="author"><name><surname>Aghababaeian</surname><given-names>Hamidreza</given-names></name><xref ref-type="aff" rid="af7-cia-13-669">7</xref></contrib><contrib contrib-type="author"><name><surname>Taghdisi</surname><given-names>Mohammad Hossein</given-names></name><xref ref-type="aff" rid="af8-cia-13-669">8</xref></contrib></contrib-group><aff id="af1-cia-13-669">
