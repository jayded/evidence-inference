<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">J Med Genet</journal-id><journal-id journal-id-type="iso-abbrev">J. Med. Genet</journal-id><journal-id journal-id-type="hwp">jmedgenet</journal-id><journal-id journal-id-type="publisher-id">jmg</journal-id><journal-title-group><journal-title>Journal of Medical Genetics</journal-title></journal-title-group><issn pub-type="ppub">0022-2593</issn><issn pub-type="epub">1468-6244</issn><publisher><publisher-name>BMJ Publishing Group</publisher-name><publisher-loc>BMA House, Tavistock Square, London, WC1H 9JR</publisher-loc></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">27834756</article-id><article-id pub-id-type="pmc">5502308</article-id><article-id pub-id-type="publisher-id">jmedgenet-2016-104178</article-id><article-id pub-id-type="doi">10.1136/jmedgenet-2016-104178</article-id><article-categories><subj-group subj-group-type="hwp-journal-coll"><subject>1506</subject></subj-group><subj-group subj-group-type="heading"><subject>Therapeutics</subject></subj-group><series-title>Original Article</series-title></article-categories><title-group><article-title>Oral pharmacological chaperone migalastat compared with enzyme replacement therapy in Fabry disease: 18-month results from the randomised phase III ATTRACT study</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Hughes</surname><given-names>Derralynn A</given-names></name><xref ref-type="aff" rid="af1">1</xref></contrib><contrib contrib-type="author"><name><surname>Nicholls</surname><given-names>Kathleen</given-names></name><xref ref-type="aff" rid="af2">2</xref></contrib><contrib contrib-type="author"><name><surname>Shankar</surname><given-names>Suma P</given-names></name><xref ref-type="aff" rid="af3">3</xref></contrib><contrib contrib-type="author"><name><surname>Sunder-Plassmann</surname><given-names>Gere</given-names></name><xref ref-type="aff" rid="af4">4</xref></contrib><contrib contrib-type="author"><name><surname>Koeller</surname><given-names>David</given-names></name><xref ref-type="aff" rid="af5">5</xref></contrib><contrib contrib-type="author"><name><surname>Nedd</surname><given-names>Khan</given-names></name><xref ref-type="aff" rid="af6">6</xref></contrib><contrib contrib-type="author"><name><surname>Vockley</surname><given-names>Gerard</given-names></name><xref ref-type="aff" rid="af7">7</xref></contrib><contrib contrib-type="author"><name><surname>Hamazaki</surname><given-names>Takashi</given-names></name><xref ref-type="aff" rid="af6">6</xref><xref ref-type="aff" rid="af8">8</xref></contrib><contrib contrib-type="author"><name><surname>Lachmann</surname><given-names>Robin</given-names></name><xref ref-type="aff" rid="af9">9</xref></contrib><contrib contrib-type="author"><name><surname>Ohashi</surname><given-names>Toya</given-names></name><xref ref-type="aff" rid="af10">10</xref></contrib><contrib contrib-type="author"><name><surname>Olivotto</surname><given-names>Iacopo</given-names></name><xref ref-type="aff" rid="af11">11</xref></contrib><contrib contrib-type="author"><name><surname>Sakai</surname><given-names>Norio</given-names></name><xref ref-type="aff" rid="af12">12</xref></contrib><contrib contrib-type="author"><name><surname>Deegan</surname><given-names>Patrick</given-names></name><xref ref-type="aff" rid="af13">13</xref></contrib><contrib contrib-type="author"><name><surname>Dimmock</surname><given-names>David</given-names></name><xref ref-type="aff" rid="af14">14</xref></contrib><contrib contrib-type="author"><name><surname>Eyskens</surname><given-names>Fran&#x000e7;ois</given-names></name><xref ref-type="aff" rid="af15">15</xref></contrib><contrib contrib-type="author"><name><surname>Germain</surname><given-names>Dominique P</given-names></name><xref ref-type="aff" rid="af16">16</xref></contrib><contrib contrib-type="author"><name><surname>Goker-Alpan</surname><given-names>Ozlem</given-names></name><xref ref-type="aff" rid="af17">17</xref></contrib><contrib contrib-type="author"><name><surname>Hachulla</surname><given-names>Eric</given-names></name><xref ref-type="aff" rid="af18">18</xref></contrib><contrib contrib-type="author"><name><surname>Jovanovic</surname><given-names>Ana</given-names></name><xref ref-type="aff" rid="af19">19</xref></contrib><contrib contrib-type="author"><name><surname>Lourenco</surname><given-names>Charles M</given-names></name><xref ref-type="aff" rid="af20">20</xref></contrib><contrib contrib-type="author"><name><surname>Narita</surname><given-names>Ichiei</given-names></name><xref ref-type="aff" rid="af21">21</xref></contrib><contrib contrib-type="author"><name><surname>Thomas</surname><given-names>Mark</given-names></name><xref ref-type="aff" rid="af22">22</xref></contrib><contrib contrib-type="author"><name><surname>Wilcox</surname><given-names>William R</given-names></name><xref ref-type="aff" rid="af23">23</xref></contrib><contrib contrib-type="author"><name><surname>Bichet</surname><given-names>Daniel G</given-names></name><xref ref-type="aff" rid="af24">24</xref></contrib><contrib contrib-type="author"><name><surname>Schiffmann</surname><given-names>Raphael</given-names></name><xref ref-type="aff" rid="af25">25</xref></contrib><contrib contrib-type="author"><name><surname>Ludington</surname><given-names>Elizabeth</given-names></name><xref ref-type="aff" rid="af26">26</xref></contrib><contrib contrib-type="author"><name><surname>Viereck</surname><given-names>Christopher</given-names></name><xref ref-type="aff" rid="af27">27</xref></contrib><contrib contrib-type="author"><name><surname>Kirk</surname><given-names>John</given-names></name><xref ref-type="aff" rid="af27">27</xref></contrib><contrib contrib-type="author"><name><surname>Yu</surname><given-names>Julie</given-names></name><xref ref-type="aff" rid="a